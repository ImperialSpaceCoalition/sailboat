    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-router-dom": "^5.3.0",
    "@apollo/client": "^3.5.0",
    "graphql": "^15.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2"
    
  },
  "engines": {
    "node": ">=14.0.0"
  },
